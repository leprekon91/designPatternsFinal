<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<GridPane hgap="8.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity"
          minWidth="-Infinity" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.fxmlControllers.AddPurchase">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="50.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="10.0" prefWidth="50.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="120.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Label fx:id="titleLbl" text="Add Insurance" textAlignment="CENTER" textFill="#2629c9" GridPane.columnIndex="1"
               GridPane.halignment="CENTER" GridPane.valignment="CENTER">
            <font>
                <Font name="System Bold" size="30.0"/>
            </font>
        </Label>
        <TextField fx:id="nameFld" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        <TextField fx:id="surnameFld" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
        <TextArea fx:id="remarksFld" prefHeight="200.0" prefWidth="200.0" promptText="Remarks" GridPane.columnIndex="1"
                  GridPane.rowIndex="8">
            <GridPane.margin>
                <Insets top="16.0"/>
            </GridPane.margin>
        </TextArea>
        <DatePicker fx:id="dateFld" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="7"/>
        <Button mnemonicParsing="false" onAction="#submitPurchase" text="Submit" GridPane.columnIndex="1"
                GridPane.halignment="CENTER" GridPane.rowIndex="9" GridPane.valignment="TOP">
            <GridPane.margin>
                <Insets top="16.0"/>
            </GridPane.margin>
        </Button>
        <Button maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false"
                onAction="#backHome" style="-fx-background-color: #2629c9;" text="&lt;" textFill="WHITE">
            <font>
                <Font name="System Bold" size="13.0"/>
            </font>
        </Button>
        <Label text="Name" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <Label text="Surname" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
        <Label text="Date" GridPane.columnIndex="1" GridPane.rowIndex="6"/>
        <Label fx:id="statusLbl" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
               minHeight="-Infinity" minWidth="-Infinity" textAlignment="CENTER" GridPane.columnIndex="1"
               GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.rowIndex="10" GridPane.valignment="CENTER"
               GridPane.vgrow="ALWAYS"/>
    </children>
    <padding>
        <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
    </padding>
</GridPane>
